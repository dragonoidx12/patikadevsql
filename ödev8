SELECT * FROM employee

UPDATE employee
SET name = 'Furkan'
WHERE id between 1 and 5

UPDATE employee
SET birthday = '2000.01.01'
WHERE name = 'Dani'

UPDATE employee
SET email = 'deneme@deneme.com.tr'
WHERE name = 'Judi'

UPDATE employee
SET name = 'Fatih'
WHERE id = 17

UPDATE employee
SET name = 'Hasan'
WHERE id = 49

DELETE FROM Employee WHERE name = 'Hasan' 
DELETE FROM Employee WHERE name = 'Fatih' 
DELETE FROM Employee WHERE name = 'Furkan' 
DELETE FROM Employee WHERE id = 28
DELETE FROM Employee WHERE id = 56
